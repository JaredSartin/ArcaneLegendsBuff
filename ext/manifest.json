{
  "name": "Arcane Legends Buff",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Have a better experience playing Arcane Legends in your browser. Get a bigger full screen experience!",
  "homepage_url": "http://jared.sart.in/albuff",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "http://account.spacetimestudios.com/arcanelegends"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://account.spacetimestudios.com/arcanelegends"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "http://account.spacetimestudios.com/arcanelegends"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
